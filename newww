<script>

	const video = {
		p2sUrl: "https://www.power2ship.com/nevada-auto-shipping", // the url of the P2S state landing page - NOT the youtube or embed url
		state: "Nevada", // full name of the state, capitalized ex: New Jersey
		youtubeUrl: "https://www.youtube.com/watch?v=H5z9Rpw2Enc", // the video's youtube url
		duration: {
			min: "1min", // total video mins
			sec: ".02sec" // total video seconds
		}
	}

	const schema = {
		"@context": "https://schema.org",
		"@type": "VideoObject",
		"@id": video.https://www.power2ship.com/nevada-auto-shipping+videoObject",
		"url": video.https://www.power2ship.com/nevada-auto-shipping,
		"name": Nevada Auto Shipping",
		"description": "If you're planning to move your vehicle to or from Nevada, it's important to choose a reliable and experienced auto shipping company that can handle your needs. Power2Ship is one such company, offering top-notch auto shipping services throughout the state of Nevada and beyond." + video.state + ". " + video.state + " residents and businesses in need of auto shipping services can trust Power2Ship for reliable and efficient service.",
